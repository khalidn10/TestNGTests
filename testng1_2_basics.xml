<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite1">
  <test thread-count="5" name="Test1">
    <classes>
      <class name="testng1.HelloWorld1">
      <methods>
        <include name="create.*"/>
      </methods>
      </class>
      <class name="testng1.HelloWorld2"/>
    </classes>
  </test>
  <test thread-count="5" name="Test2">
    <classes>
      <class name="testng2.HelloWorld3">
      	<methods>
      	  <exclude name="create.*"/>
      	</methods>
      </class>
      <class name="testng2.HelloWorld4"/>
    </classes>
  </test>
</suite>
